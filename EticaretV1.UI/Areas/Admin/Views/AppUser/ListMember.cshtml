
@{
    ViewBag.Title = "ListMember";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
@model List<EticaretV1.Model.Option.AppUser>


<div class="col-md-12">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Kullanıcı Adı</th>
                <th>Yetki Seviyesi</th>
                <th>Email</th>
                <th>Fotoğraf</th>
                <th colspan="1">Güncelle</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.UserName</td>
                    <td>@item.Role</td>
                    <td>@item.Email</td>
                    <td><img src="@item.ImagePath" style="display:block" width="30" height="30" /></td>
                    <td>
                        <a href="/Admin/AppUser/Update/@item.Id">Güncelle</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>




